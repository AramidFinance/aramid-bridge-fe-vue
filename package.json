    "@walletconnect/modal": "^2.6.2",
    "@walletconnect/sign-client": "^2.15.1",
    "@web3modal/ethers": "^5.1.3",
    "algosdk": "^2.9.0",
    "base64url": "^3.0.1",
    "bignumber.js": "^9.1.2",
